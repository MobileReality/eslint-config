            "optional": true
        },
        "eslint-plugin-jsx-a11y": {
            "optional": true
        },
        "eslint-plugin-react": {
            "optional": true
        },
        "eslint-plugin-react-hooks": {
            "optional": true
        },
        "eslint-plugin-react-native": {
            "optional": true
        },
        "eslint-plugin-simple-import-sort": {
            "optional": true
        }
    }
