{
    "name": "@mobile-reality/eslint-config",
    "version": "0.10.1",
    "description": "ESLint's configuration of Mobile Reality Team",
    "engines": {
        "node": "^18.15.0"
    },
    "repository": "https://github.com/MobileReality/eslint-config",
    "author": "Mobile Reality <biuro@mobilereality.pl> (https://mobilereality.pl)",
    "contributors": [
        {
            "name": "@irekrog",
            "url": "https://github.com/irekrog"
        },
        {
            "name": "Mateusz Tkacz",
            "url": "https://github.com/CatchMe2"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MobileReality/eslint-config/issues"
    },
    "homepage": "https://github.com/MobileReality/eslint-config/#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "keywords": [
        "eslint",
        "config",
        "eslint config",
        "mobilereality",
        "mobile reality"
    ],
    "scripts": {
        "release": "release-it",
        "release:beta": "release-it --preRelease=beta --no-git.requireUpstream",
